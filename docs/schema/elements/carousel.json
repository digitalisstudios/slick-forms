{
    "metadata": {
        "type": "carousel",
        "label": "Carousel",
        "icon": "bi-collection-play",
        "description": ""
    },
    "usage": {
        "model": "SlickFormLayoutElement",
        "method": "create",
        "example": {
            "slick_form_id": 1,
            "element_type": "carousel",
            "element_id": "hero-carousel",
            "order": 0,
            "settings": {
                "direction": "horizontal",
                "speed": 300,
                "loop": true,
                "slides_per_view": 1,
                "space_between": 30,
                "autoplay": {
                    "enabled": true,
                    "delay": 3000
                },
                "navigation": {
                    "enabled": true
                },
                "pagination": {
                    "enabled": true,
                    "type": "bullets",
                    "clickable": true
                }
            }
        }
    },
    "properties": {
